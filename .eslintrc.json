{
  "extends": ["react-app","airbnb", "airbnb-typescript", "plugin:jsx-a11y/recommended", "plugin:prettier/recommended"],
  "plugins": ["jsx-a11y"],
   "parserOptions": {
     "project": "./tsconfig.json"
   },
  "rules": {
    "react/jsx-filename-extension": [1, {"extensions":  [".js", ".tsx"]}],
    "comma-spacing": 2,
    "comma-dangle": [2, "never"],
    "indent": ["error", 2, {"SwitchCase": 1}],
    "keyword-spacing": 1,
    "max-len": [1, 180, 4],
    "quotes": [2, "single"],
    "semi": 2,
    "space-before-function-paren": 1,
    "spaced-comment": 2,
    "object-shorthand": 2,
    "object-curly-spacing": [2, "always"],
    "prefer-const": 2,
    "valid-typeof": 2,
    "no-const-assign": 2,
    "no-console": 2,
    "no-debugger": 2,
    "no-duplicate-imports": 2,
    "no-empty": 2,
    "no-extra-bind": 2,
    "no-extra-semi": 1,
    "no-eval": 2,
    "no-multi-spaces": 2,
    "no-nested-ternary": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-undefined": 0,
    "no-unreachable": 1,
    "no-unused-expressions": 0,
    "no-unused-vars": 2,
    "no-useless-constructor": 2,
    "no-with": 2,
    "react/no-unused-prop-types": true,
    "react/prop-types": 0,
    "@typescript-eslint/ban-ts-comment": false,
    "@typescript-eslint/no-unsafe-argument": false,
    "@typescript-eslint/no-explicit-any": false,
    "@typescript-eslint/no-misused-promises ": false,
    "@typescript-eslint/no-unused-expressions": false,
    "@typescript-eslint/await-thenable": "off",
    "import/no-extraneous-dependencies": ["error",  {"devDependencies": true}],
    "react/jsx-props-no-spreading": "off",
    "@typescript-eslint/comma-dangle": "off",
    "@typescript-eslint/ban-types": "off",
    "@typescript-eslint/no-unsafe-assignment": "off",
    "@typescript-eslint/restrict-template-expressions": ["error", 1,{ "allowAny": true }],
    "@typescript-eslint/no-unsafe-member-access": "error",
    "react/jsx-no-undef": 2,
    "func-names": "off",
    "react/jsx-indent": [2, 2],
    "eslint/func-names": false,
    "react/function-component-definition": false,
    "@typescript-eslint/no-misused-promises": false,
    "react/jsx-props-no-spreading": false,
    "react/jsx-tag-spacing": [2, {"beforeSelfClosing": "always"} ],
    "react/jsx-closing-bracket-location": 2,
    "react/jsx-indent-props": [2, 2],
    "react/no-string-refs": 1,
    "react/no-did-mount-set-state": 1,
    "react/no-did-update-set-state": 1,
    "react/jsx-one-expression-per-line": ["enabled", { "allow": "none"}],
    "react-native/split-platform-components": 2,
    "prettier/prettier": "off",
    "object-curly-newline": "off"
  },
  "settings": {
    "react": {
      "version": "18.1.0"
    }
  }
}